<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zuofw.mapper.TopicDOMapper">
    <select id="selectNameByPrimaryKey" parameterType="java.lang.Long" resultType="string">
        select
            name
        from t_topic
        where id = #{id,jdbcType=BIGINT}
    </select>

</mapper>